{
  "extends": "./tsconfig.json",
  "typedocOptions": {
    "out": "docs",
    "entryPoints": ["src/index.ts"],
    "plugin": "typedoc-plugin-markdown",
    "excludePrivate": true,
    "excludeProtected": true,
    "disableSources": true,
    "readme": "none"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
