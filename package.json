    "types"
  ],
  "types": "types",
  "devDependencies": {
    "@types/node": "^10.17.60",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "tap": "^16.3.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "build": "tsc -b tsconfig-build.json",
