{
  "name": "sequelize-pool",
  "description": "Resource pooling for Node.JS",
  "version": "8.0.0",
  "author": "Sushant <sushantdhiman@outlook.com>",
  "keywords": [
    "pool",
    "pooling",
    "throttle",
